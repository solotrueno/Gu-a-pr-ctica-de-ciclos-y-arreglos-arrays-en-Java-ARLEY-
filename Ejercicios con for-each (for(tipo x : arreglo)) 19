public class ArrayForEach {

    public static void main(String[] args) {
        
        int[] numeros = {5, 10, 15, 20};
        
        System.out.println("--- Elementos del Array 'numeros' ---");
        for (int numero : numeros) {
            
            System.out.println("NÃºmero: " + numero);
        }
        
        System.out.println("-------------------------------------");
    }
}
