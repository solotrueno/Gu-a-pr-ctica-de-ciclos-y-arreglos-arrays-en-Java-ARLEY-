public class NumerosDoWhile {

    public static void main(String[] args) {
        int contador = 1;
        
        System.out.println("Mostrando n√∫meros del 1 al 5 (usando do-while):");
        
        do {
            System.out.println(contador);
            
            contador++; 
        
        } while (contador <= 5);
        
        System.out.println("Proceso completado.");
    }
}
