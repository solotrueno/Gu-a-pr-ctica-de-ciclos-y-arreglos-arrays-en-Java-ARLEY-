public class NombresPorLinea {

    public static void main(String[] args) {
        
        String[] nombres = {"Andrea", "Benito", "Cecilia", "David"};
        
        System.out.println("--- Lista de Nombres ---");
        for (int i = 0; i < nombres.length; i++) {
            System.out.println(nombres[i]);
        }
        
        System.out.println("------------------------");
    }
}
